{
  "extends: "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere" : true
  },
  "rules" : {
      "no-unused-vars" : "off",
      "no-undef" : "off",
      "prefer-destructuring" : "off",
      "react/jsx-filename-extension" : "off",
      "react/no-array-index-key" : "off",
      "react-hooks/exhaustive-deps" : "off",
      "import/no-named-as-default" : "off"
  }
}
